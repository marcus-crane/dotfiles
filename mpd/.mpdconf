music_directory    "$HOME/Music"
playlist_directory "$HOME/.mpd/playlists"
db_file            "$HOME/.mpd/database"
pid_file           "$HOME/.mpd/pid"
state_file         "$HOME/.mpd/state"
port               "6600"
auto_update        "yes"

audio_output {
    type           "fifo"
    name           "FIFO"
    path           "/tmp/mpd.fifo"
    format         "44100:16:2"
}
