# This Brewfile contains the bare minimum tools that
# I use regularly.
#
# It's intended for use when I use a different laptop
# temporarily and want to get up and running as quick
# as possible
tap "homebrew/cask"

# cli tools
brew "azure-cli"
brew "bash"
brew "curl"
brew "fzf"
brew "htop"
brew "jq"
brew "neovim"
brew "ripgrep"
brew "wget"
brew "zsh"
