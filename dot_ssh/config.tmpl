Host *
  ControlMaster auto
  ControlPath ~/.ssh-sockets/%r
  ControlPersist 600
  ServerAliveInterval 30
  ServerAliveCountMax 3
  AddKeysToAgent yes
  UseKeychain yes
  Compression yes

Host netocean
  HostName 192.168.1.111
  User marcus

Host izumo
  HostName 192.168.1.244
  User marcus

{{- if eq (output "sysctl" "-n" "hw.model") "MacBookPro16,4" }}
Include ~/Code/work/home/.ssh/config
{{- end }}
